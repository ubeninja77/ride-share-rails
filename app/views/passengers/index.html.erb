<main>
  <h1 class="passenger-list">All Passengers</h1>

  <section id="passengers-trip-list">
    <ul>
      <% @passengers.each do |passenger| %>
          <li>
            <header><strong><%= link_to passenger.name, passenger_path(passenger.id) %> </header></strong>
          </li>
      <% end %>
    </ul>
  </section>
</main> 